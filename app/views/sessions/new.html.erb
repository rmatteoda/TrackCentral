<% provide(:title, "Entrar") %>

<div class="span9">
     <%= form_for(:session, :url => sessions_path) do |f| %>
      
      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password, 'Clave' %>
      <%= f.password_field :password %>

    <div class="actions">
 
      <%= f.submit "Entrar", :class => "actions btn-small btn-primary" %>
    </div>
    <% end %>
  </div>